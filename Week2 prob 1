#include <iostream>
#include <set>

int main() {
    int n;
    std::cin >> n;

    std::set<int> distinctValues;
    for (int i = 0; i < n; ++i) {
        int num;
        std::cin >> num;
        distinctValues.insert(num);
    }

    int numDistinct = distinctValues.size();
    std::cout << numDistinct << std::endl;

    return 0;
}
